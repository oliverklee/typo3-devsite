{
    "name": "oliverklee/typo3-devsite",
    "description": "TYPO3 extension that contains the basics of a site which I use for developing in TYPO3.",
    "type": "typo3-cms-extension",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Oliver Klee",
            "role": "Developer",
            "homepage": "https://www.oliverklee.de"
        }
    ],
    "require": {
        "php": ">=5.6.0,<=7.1.99",
        "typo3/cms-core": ">=6.2.0,<=8.9.99"
    },
    "replace": {
        "typo3-devsite": "self.version",
        "typo3-ter/typo3-devsite": "self.version"
    }
}